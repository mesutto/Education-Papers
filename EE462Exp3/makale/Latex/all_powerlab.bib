doi = {10.1109/TIE.2016.2519321},
file = {:D$\backslash$:/mymendeley/2016/The Essential Role and the Continuous Evolution of Modulation Techniques for Voltage-Source Inverters in the Past, Present, and Future P.pdf:pdf},
isbn = {0278-0046},
