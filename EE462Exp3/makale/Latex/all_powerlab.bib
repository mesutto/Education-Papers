@article{Ulbig2015,
abstract = {Large-scale deployment of Renewable Energy Sources (RES) has led to significant generation shares of variable RES in power systems worldwide. RES units, notably inverter-connected wind turbines and photovoltaics (PV) units that do not provide rotational inertia, are effectively displacing conventional generators and their rotating machinery. This has implications for frequency dynamics and power system operation: Since frequency dynamics are faster in power systems with low rotational inertia, this can lead to large transient frequency and power oscillations in multi-area power systems. Grid topology also has a key role for frequency dynamics as meshed grids are more resilient to disturbances than non-meshed grids. This paper investigates the role of low rotational inertia and different grid topology setups on power system stability for a three-area power system. The presented contributions, both analytical and simulation-based insights, extend previous results for a two-area power system.},
author = {Ulbig, Andreas and Borsche, Theodor S. and Andersson, G{\"{o}}ran},
doi = {10.1016/j.ifacol.2015.12.436},
file = {:D$\backslash$:/mymendeley/2015/Analyzing Rotational Inertia, Grid Topology and their Role for Power System Stability{\_}Ulbig, Borsche, Andersson{\_}2015.pdf:pdf},
issn = {24058963},
journal = {IFAC-PapersOnLine},
keywords = {Frequency Control,Grid Topology,Power System Stability,Rotational Inertia},
number = {30},
pages = {541--547},
title = {{Analyzing Rotational Inertia, Grid Topology and their Role for Power System Stability}},
volume = {48},
year = {2015}
}
@article{Wang2015d,
abstract = {This paper investigates an improved active power control method for variable speed wind turbine to enhance the inertial response and damping capability during transient events. The optimized power point tracking (OPPT) controller, which shifts the turbine operating point from the maximum power point tracking (MPPT) curve to the virtual inertia control (VIC) curves according to the frequency deviation, is proposed to release the “hidden” kinetic energy and provide dynamic frequency support to the grid. The effects of the VIC on power oscillation damping capability are theoretically evaluated. Compared to the conventional supplementary derivative regulator-based inertia control, the proposed control scheme can not only provide fast inertial response, but also increase the system damping capability during transient events. Thus, inertial response and power oscillation damping function can be obtained in a single controller by the proposed OPPT control. A prototype three-machine system containing two synchronous generators and a PMSG-based wind turbine with 31{\%} of wind penetration is tested to validate the proposed control strategy on providing rapid inertial response and enhanced system damping.},
author = {Wang, Yi and Meng, Jianhui and Zhang, Xiangyu and Xu, Lie},
doi = {10.1109/TSTE.2015.2394363},
file = {:D$\backslash$:/mymendeley/2015/Control of PMSG-Based Wind Turbines for System Inertial Response and Power Oscillation Damping{\_}Wang et al.{\_}2015.pdf:pdf},
issn = {19493029},
journal = {IEEE Transactions on Sustainable Energy},
keywords = {-frequency sensitive control,Frequency support,permanent magnet synchronous generator (PMSG),power oscillation damping,variable speed wind turbine,virtual inertia control (VIC)},
number = {2},
pages = {565--574},
title = {{Control of PMSG-Based Wind Turbines for System Inertial Response and Power Oscillation Damping}},
volume = {6},
year = {2015}
}
