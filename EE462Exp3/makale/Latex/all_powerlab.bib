keywords = {Degradation,Insulated gate bipolar transistors,Inverters,Logic gates,Multichip modules,Power conversion,Static power converters,Thermal degradation},
title = {{Condition Monitoring: A Decade of Proposed Techniques}},
year = {2015}
}
