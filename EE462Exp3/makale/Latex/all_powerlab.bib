@article{Berzoy2017,
abstract = {—This paper presents an improved complex-vector dy-namic model for a three-phase induction machine (IM) with sta-tor interturn short-circuit fault. The paper proposes an accurate and meaningful approach to the faulty stator leakage inductance and describes a nonlinear phenomenon in the faulty circuit. The asymmetric model is developed in detail and represented in state-space (SS) form with the flux-linkages as the SS variable. Based on the proposed model, steady-state complex-vector, operational, and sequence component equivalent circuits are derived for fault detection and identification purposes. For reasons of verification, the model is simulated in MatLab environment and experimen-tally corroborated for a 1HP three-phase squirrel-cage IM. The simulations and experiments are compared at no-load and loading condition for both transient and steady-state response. The per-manent regime analysis studies the model with respect to the fault severity factor, fault resistance, and load variation. The proposed model shows high correlation with the experimental results. Index Terms—Complex-vector, fault detection and identifica-tion, induction motor, interturn short-circuit (ITSC) fault, state-space modeling.},
author = {Berzoy, Alberto and Mohamed, Ahmed A S and Mohammed, Osama},
doi = {10.1109/TIA.2016.2632068},
file = {:D$\backslash$:/mymendeley/2017/Complex-Vector Model of Interturn Failure in Induction Machines for Fault Detection and Identification{\_}Berzoy, Mohamed, Mohammed{\_}2017.pdf:pdf},
journal = {IEEE TRANSACTIONS ON INDUSTRY APPLICATIONS},
number = {3},
title = {{Complex-Vector Model of Interturn Failure in Induction Machines for Fault Detection and Identification}},
volume = {53},
year = {2017}
}
