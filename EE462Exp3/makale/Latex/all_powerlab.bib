issn = {00939994},
journal = {IEEE Transactions on Industry Applications},
number = {6},
pages = {4065--4076},
title = {{Reactive power injection strategies for single-phase photovoltaic systems considering grid requirements}},
volume = {50},
year = {2014}
}
