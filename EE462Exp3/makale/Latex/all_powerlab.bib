@article{Hsieh2001,
abstract = {This paper explores the factors that affect the accuracy of$\backslash$nnumerical analysis of railguns with motion using Electromechanical$\backslash$nAnalysis Program in Three Dimensions (EMAP3D), a Lagrangian finite$\backslash$nelement method (FEM) code that models thermal and electromagnetic$\backslash$ndiffusion into conductors with moving interfaces. In situations$\backslash$ninvolving sliding electric contact between conductors, EMAP3D solves a$\backslash$nseries of velocity-dependent diffusion problems in which the armature$\backslash$nmoves to different axial positions that satisfy the equations of motion$\backslash$nfar the armature. This paper develops two relationships between time$\backslash$nstep size and solution accuracy. One relationship is a lower bound on$\backslash$ntime step size, based on the need for linear brick elements to represent$\backslash$naccurately the exponential-like spatial variation of current density as$\backslash$ncurrents diffuse into conductors. The other relationship is an upper$\backslash$nbound, which limits the motion of the armature to distances on the scale$\backslash$nof the current distribution around the armature. Because the upper bound$\backslash$ndecreases with increasing velocity, the two bounds eventually converge,$\backslash$nat which point accurate solutions to problems involving motion are no$\backslash$nlonger possible for given mesh dimensions. The velocity at which$\backslash$naccurate solutions are possible can be increased by increasing the$\backslash$nresolution of the mesh. At present, a practical limit to simulating$\backslash$nrealistic railgun problems is less than 500 m/s. Because this limit is$\backslash$nset by rapidly advancing state-of-the-art computer hardware, the$\backslash$nprospects for achieving higher velocities in the near future are good$\backslash$n},
