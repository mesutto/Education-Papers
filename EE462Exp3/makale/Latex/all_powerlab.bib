journal = {IEEE Transactions on Sustainable Energy},
keywords = {Frequency control,isolated power systems,power system dynamic stability,power system security,solar power generation,supercapacitors,wind power generation},
number = {4},
pages = {931--939},
title = {{Dynamic frequency control support by energy storage to reduce the impact of wind and solar generation on isolated power system's inertia}},
volume = {3},
year = {2012}
}
