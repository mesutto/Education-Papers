keywords = {Index Terms—Diesel driven generators,diesel engines,imbal-ance,misfiring,power quality},
number = {3},
title = {{A Monitoring System for Diesel Engine Driven Generators Based on Electric Power Output Oscillation Assessment}},
volume = {53},
year = {2017}
}
@article{Salameh2017,
abstract = {—This paper describes an original statistical approach for the lifespan modeling of electric machine insulation materi-als. The presented models aim to study the effect of three main stress factors (voltage, frequency, and temperature) and their in-teractions on the insulation lifespan. The proposed methodology is applied to two different insulation materials tested in partial dis-charge regime. Accelerated ageing tests are organized according to experimental optimization methods in order to minimize the experimental cost while ensuring the best model accuracy. In ad-dition to classical parametric models, the life–stress relationship is expressed through original nonparametric and hybrid models that have never been investigated in insulation aging studies before. These two models present the original contribution of this paper. For each material, models are computed from organized sets of experiments and applied on a randomly configured test set for va-lidity checking. The different models are evaluated and compared in order to define their optimal use.},
author = {Salameh, Farah and Picot, Antoine and Chabert, Marie and Maussion, Pascal},
doi = {10.1109/TIA.2016.2635100},
file = {:D$\backslash$:/mymendeley/2017/Parametric and Nonparametric Models for Lifespan Modeling of Insulation Systems in Electrical Machines{\_}Salameh et al.{\_}2017.pdf:pdf},
journal = {IEEE TRANSACTIONS ON INDUSTRY APPLICATIONS},
keywords = {Index Terms—Accelerated aging,design optimization,electric machines,insulation,lifetime estimation,model checking,model-ing,partial discharges (PD),regression analysis,stress},
number = {3},
title = {{Parametric and Nonparametric Models for Lifespan Modeling of Insulation Systems in Electrical Machines}},
volume = {53},
year = {2017}
