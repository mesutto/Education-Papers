doi = {10.1109/TIE.2016.2521338},
file = {:D$\backslash$:/mymendeley/2016/A Robust Predictive Current Controller for PMSM Drives{\_}Turker, Buyukkeles, Bakan{\_}2016.pdf:pdf},
isbn = {0278-0046},
issn = {02780046},
journal = {IEEE Transactions on Industrial Electronics},
keywords = {PMSM,dead-beat current control,digital current control,robust prediction},
number = {6},
