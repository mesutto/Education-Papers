journal = {IEEE Transactions on Magnetics},
keywords = {Coil,Electromagnetic compatibility,Electromagnetic field,Magnetic field,Payloads,Railgun,Spark gap},
number = {1},
pages = {578--583},
title = {{The use of electronic components in railgun projectiles}},
volume = {45},
year = {2009}
}
