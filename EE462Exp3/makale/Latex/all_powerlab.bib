@article{Besdel2008,
abstract = {SSPAs are largely used in Navigation and Telecom satellites. Some Payloads may require more than 100 off SSPAs. In this case, most of the payload power is used to supply these SSPAs. EPCs (Electronic Power Conditioner) supplying these SSPAs are then high performance DC/DC converters. Mass and efficiency are the 2 drivers for these DC/DC converters as they could be numerous on a satellite. Usually, these EPCs were presented as modules outside of the SSPA. It has appeared that some mass can be saved if the EPC could be implemented in the same chassis as the SSPA. This paper details the solutions developed for a 60W EPC to be delivered as a PCB without mechanical frame. Parts are selected to cope with thermal management of the PCB embedded in the SSPA host chassis. EPC switching frequency has been optimized to reduce losses without increasing too largely the size of the filtering elements. The main performances achieved by this 60W EPC are detailed and illustrated by measurements recorded on one EM board.},
author = {Besdel, Patrick and Soubrier, Laurent and Limaux, Olivier},
file = {:D$\backslash$:/mymendeley/2008/HIGH EFFICIENCY, LOW MASS, EPC FOR SSPA{\_}Besdel, Soubrier, Limaux{\_}2008.pdf:pdf},
