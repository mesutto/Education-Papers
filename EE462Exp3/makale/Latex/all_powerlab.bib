issn = {00189464},
journal = {IEEE Transactions on Magnetics},
keywords = {Accuracy,Diffusion,Time-step size,Velocity skin effect},
number = {1 I},
pages = {416--420},
title = {{Numerical modeling of the velocity skin effects: An investigation of issues affecting accuracy}},
volume = {37},
year = {2001}
}
