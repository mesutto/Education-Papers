@article{,
abstract = {F AULT diagnosis of electric machines, power electronics, and drives has been the object of a significant amount of re-search work during the last few decades. Reducing maintenance costs and preventing unscheduled downtimes, which cause pro-duction and economic losses, are a priority for manufacturers and operators. Correct diagnosis and early detection of incipi-ent faults lead to reduce maintenance costs and downtime for the process under consideration, and in many cases also prevent harmful and sometimes disruptive failures. Moreover, the comprehension of the fault occurrence con-ditions, and the failure detection and location, are the starting point for the study of effective fault-tolerant techniques, since the latter must always include failure detection, isolation of failed component, and component reconfiguration. This Special Issue on Fault Diagnosis of Electric Machines, Power Electronics, and Drives aims to offer an insight into the state-of-the-art of diagnostics and condition monitoring of elec-trical machines, power electronics, adjustable speed drives, and related areas. One hundred papers were received for this Special Issue, from which 54 papers were accepted for publication. The papers are divided into six groups, covering the following topics: 1) electrical machines including induction motor faults (sta-tor windings, air gap, rotor cage, and bearings), permanent magnet synchronous machine faults (interturn, phase-to-phase, or phase-to-ground short circuits, and demag-netization), multiphase permanent magnet assisted syn-chronous reluctance motor faults, and transformers turn-to-turn faults; 2) power electronics including fault-tolerant T-type multi-level inverters, modular multilevel converters for HVdc transmission networks, grid side converters for intercon-necting renewable energy sources, interleaved boost dc– dc converters for solar photovoltaic systems, silicon-based IGBTs, SiC MOSFET power modules, and electrostatic discharge testing; 3) electrical drives including permanent magnet syn-chronous motor drives failures (IGBTs, current sensors, windings insulation, and mechanical shaft), ac motor drive as a diagnostic sensor of gear faults, open-switch faults in T-type inverter-fed induction motor drives, fault-tolerant position sensor controller for switched reluctance motor Digital Object Identifier 10.1109/TIA.2017.2688758 This work was supported by the Portuguese Foundation for Science and Technology under Project SFRH/BSAB/118741/2016. drives, and high resistance grounding faults in adjustable speed drive systems; 4) tools for diagnostics including phaselet transform, wavelet transform, multiresolution wavelet packet decomposition, winding function approach; multiscale filtering spectrum, hypothesis testing, support vector machine, large memory storage retrieval neural networks, AdaBoost algorithm, multivariate quality control charts, high-dimensional fea-ture reduction, and higher order lumped parameter thermal networks; 5) materials for electrical machines including insulation ma-terials (accelerated aging, partial discharges, polarization current, and lifetime estimation), and retaining rings; 6) predictive maintenance including synchronous genera-tors, electric submersible pumps, diesel engine driven generators, and reliability-centered maintenance.},
annote = {A special issue on CM and FD and it is hot. Last issue of IEEE transactions on industry applications. View the issue for full new journals.},
doi = {10.1109/TIA.2017.2688758},
file = {:D$\backslash$:/mymendeley/2017/Guest Editorial Special Issue on Fault Diagnosis of Electric Machines, Power Electronics, and Drives{\_}Unknown{\_}2017.pdf:pdf},
journal = {IEEE TRANSACTIONS ON INDUSTRY APPLICATIONS},
number = {3},
title = {{Guest Editorial Special Issue on Fault Diagnosis of Electric Machines, Power Electronics, and Drives}},
volume = {53},
year = {2017}
}
