keywords = {PV system integration,electric current control,flexible active power control,grid-connected multifunctional photovoltaic invert,inner current control loop,intelligent services,invertors,low voltage ride-through,photovoltaic power systems,photovoltaic system integration,power control,power control flexibilities,power control strategy,power electricity generation,reactive power compensation,reliability-oriented thermal management,single-phase PQ theory,solar PV energy},
number = {4},
