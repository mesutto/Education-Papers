journal = {IEEE Transactions on Industrial Electronics},
keywords = {Modulation,Multilevel systems},
title = {{The Essential Role and the Continuous Evolution of Modulation Techniques for Voltage-Source Inverters in the Past, Present, and Future Power Electronics}},
