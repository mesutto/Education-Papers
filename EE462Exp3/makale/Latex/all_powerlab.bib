journal = {Proceedings of the Institution of Electrical Engineers},
number = {8},
pages = {1387},
title = {{Effects of eddy currents in transformer windings}},
url = {http://digital-library.theiet.org/content/journals/10.1049/piee.1966.0236},
volume = {113},
year = {1966}
}
